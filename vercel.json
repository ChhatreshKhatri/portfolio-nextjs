{
    "github": {
      "silent": true,
      "autoAlias": false
    },
    "git": {
        "deploymentEnabled": {
          "master": false
        }
      },
      "headers":[
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        }
      ]
  }