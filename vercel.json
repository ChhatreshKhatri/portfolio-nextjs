{
  "github": {
    "silent": true,
    "autoAlias": false
  },
  "git": {
    "deploymentEnabled": {
      "master": false
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "noindex"
        }
      ]
    }
  ]
}
